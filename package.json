{
  "name": "momentum-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": ">= 10.7.0 < 11.0.0",
    "node": ">=18.20.4 < 20.0.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json dist",
    "dev": "vite",
    "start-dev": "env-cmd -f .env.development npm install && vite",
    "start-prod": "env-cmd -f .env.production npm run clean && npm install && vite build",
    "build": "vite build",
    "lint": "eslint .",
    "format": "prettier --write ./src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.2.5",
    "@mui/icons-material": "^6.4.7",
    "@mui/material": "^6.4.7",
    "@reduxjs/toolkit": "^2.6.0",
    "env-cmd": "^10.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.0",
    "tailwindcss": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "postcss": "^8.5.3",
    "prettier": "3.5.3",
    "tailwindcss": "^4.0.12",
    "vite": "^6.2.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
